@import Tooltip
.tooltip
    h3
        font-size 1.1em
        margin 0px
    p
        margin 0px
svg
    .department
        rect.old
            fill #6b486b
            &:hover
                outline 2px solid rgba(lighten(#6b486b, 50%), 0.8)
        rect.new
            fill #98abc5
            &:hover
                outline 2px solid rgba(darken(#98abc5, 50%), 0.7)
